{
  "name": "splittttter-flow",
  "version": "2.0.0",
  "description": "Splittttter.Flow - Professional Video Splitting Studio with black & golden theme",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "install:all": "npm install && npm run install:server && npm run install:client",
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "build": "cd client && npm run build",
    "clean": "rimraf server/uploads/* client/dist server/node_modules client/node_modules",
    "start": "npm run dev"
  },
  "keywords": ["splittttter-flow", "video", "splitter", "ffmpeg", "professional", "black", "golden"],
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5"
  }
}
